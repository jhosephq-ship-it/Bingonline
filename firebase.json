{
  "hosting": [
    {
      "target": "dev",
      "public": "public",
      "ignore": [
        "**/.*",
        "**/node_modules/**",
        "../scripts/**",
        "../*.js",
        "../__tests__/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "https://www.juega-online.com",
          "type": 301
        }
      ]
    },
    {
      "target": "stg",
      "public": "public",
      "ignore": [
        "**/.*",
        "**/node_modules/**",
        "../scripts/**",
        "../*.js",
        "../__tests__/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "target": "prod",
      "public": "public",
      "ignore": [
        "**/.*",
        "**/node_modules/**",
        "../scripts/**",
        "../*.js",
        "../__tests__/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  ]
}
